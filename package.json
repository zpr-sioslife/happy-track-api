{
  "name": "express-api-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "tsc-watch --outDir ./dist --onSuccess \"node -r ./tsconfig-paths-bootstrap.js ./dist/index.js\"",
    "start:nodemon": "nodemon --watch src --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm run lint && npm run build",
    "serve": "node -r ./tsconfig-paths-bootstrap.js ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrunoAndreDias/express-api-setup.git"
  },
  "keywords": [],
  "author": "Bruno Dias <baad@sapo.pt>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BrunoAndreDias/express-api-setup/issues"
  },
  "homepage": "https://github.com/BrunoAndreDias/express-api-setup#readme",
  "dependencies": {
    "argon2": "^0.25.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@typegoose/typegoose": "^6.2.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.6",
    "@types/mongoose": "^5.5.41",
    "@types/node": "^13.1.7",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tsc-watch": "^4.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.0-beta"
  }
}
